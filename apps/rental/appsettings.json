{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME_RENTAL};Username=${DB_USERNAME};Password=${DB_PASSWORD};SSL Mode=Require;Trust Server Certificate=true"
  },
  "GrpcServices": {
    "UserService": "${USER_GRPC_URL:http://localhost:5002}",
    "DeviceService": "${DEVICE_GRPC_URL:http://localhost:5003}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}